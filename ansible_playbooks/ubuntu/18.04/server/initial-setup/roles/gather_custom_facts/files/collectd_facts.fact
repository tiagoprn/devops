#!/bin/bash

ls /usr/sbin/collectd > /dev/null 2>&1

# If there is an error on the previous bash command, collectd is not installed
# below must result in a json formatted dict, so ansible can use it
if [ $? -ne 0 ] ; then
    echo "{\"has_collectd_installed\": false}"
else
    echo "{\"has_collectd_installed\": true}"
fi

