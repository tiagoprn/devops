#!/bin/bash

ls /var/log/get-sysctl_passed.txt > /dev/null 2>&1

# If there is an error on the previous bash command, get-sysctl role has not run before
# below must result in a json formatted dict, so ansible can use it
if [ $? -ne 0 ] ; then
    echo "{\"has_get-sysctl_passed\": false}"
else
    echo "{\"has_get-sysctl_passed\": true}"
fi

