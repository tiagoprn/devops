% multipass

# Select vm name
multipass list | fzf | awk '{print $1}' | xclip -i -selection clipboard

# Select vm ip
multipass list | fzf | awk '{print $3}' | xclip -i -selection clipboard

# List:
multipass list

# Stop:
multipass stop <vm>

# Start:
multipass start <vm>

# Delete:
multipass delete <vm>

# Purge deleted: :
multipass purge
