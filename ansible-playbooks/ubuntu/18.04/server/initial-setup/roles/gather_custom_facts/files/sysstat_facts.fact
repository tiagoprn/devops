#!/bin/bash

ls /var/log/sysstat_passed.txt > /dev/null 2>&1

# If there is an error on the previous bash command, sysstat role has not run before
# below must result in a json formatted dict, so ansible can use it
if [ $? -ne 0 ] ; then
    echo "{\"has_sysstat_passed\": false}"
else
    echo "{\"has_sysstat_passed\": true}"
fi

